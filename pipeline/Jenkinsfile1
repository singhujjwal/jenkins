pipeline {
    agent any
    stages {
        stage('Example Build') {
            // Can have a docker agent at stage level as well
            // agent { docker 'maven:3.8.1-adoptopenjdk-11' } 
            steps {
                echo 'Hello, Maven'
                sh 'Running any shell step here'
            }
        }
        stage('Example Test') {
            steps {
                echo 'Hello, JDK, if there are any.'
                sh 'java -version'
            }
        }
    }
}